 <dialogOption id="protests" operator="Caller:" emotions="briefly" textpl="Na demonstracji przy [[ADDRESS]]!" texten="At the protest at [[ADDRESS]]!" default="whatprotest" option2="back" option3="details" actionOnEnd="actionSetLocation;" />
 <dialogOption id="details" operator="Operator:" textpl="{GDZIE DOKŁADNIE?} Jak możemy was znaleźć w tłumie?" texten="{WHERE EXACTLY?} How can we find you in the crowd?" default="monu" />
 <dialogOption id="monu" operator="Caller:" emotions="briefly" textpl="Jesteśmy tuż przy fontannie, widać ją z daleka." texten="We're by the fountain, you can see it from a distance." default="back" option2="how" />
